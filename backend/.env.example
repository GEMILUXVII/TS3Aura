# 环境配置
NODE_ENV=development
PORT=3001

# 服务器配置
SERVER1_HOST=ts1.kcpo.us
SERVER1_PORT=10012
SERVER1_USERNAME=serveradmin
SERVER1_PASSWORD=your_password_here

SERVER2_HOST=ts2.kcpo.us
SERVER2_PORT=10011
SERVER2_USERNAME=serveradmin
SERVER2_PASSWORD=your_password_here

# 数据库配置 (如果需要)
# DATABASE_URL=mongodb://localhost:27017/teamspeak

# 邮件配置 (用于反馈功能)
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your_email@example.com
SMTP_PASS=your_smtp_password

# 安全配置
JWT_SECRET=your_jwt_secret_here
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# 联系信息
CONTACT_QQ=your_qq_number
CONTACT_QQ_GROUP=your_qq_group
CONTACT_EMAIL=your_email@example.com
CONTACT_TELEGRAM=https://t.me/your_username

# 日志配置
LOG_LEVEL=info
LOG_FILE=logs/app.log

# CORS 配置
ALLOWED_ORIGINS=http://localhost:3000,https://kcpo.us

# 缓存配置
CACHE_TTL=30000
STATS_CACHE_TTL=300000
